{
  "extends": ["standard"],
  "plugins": [
      "svelte3"
  ],
  "overrides": [
      {
          "files": ["**/*.svelte"],
          "processor": "svelte3/svelte3"
      }
  ],
  "env": {
      "es6": true,
      "browser": true,
      "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  }
}